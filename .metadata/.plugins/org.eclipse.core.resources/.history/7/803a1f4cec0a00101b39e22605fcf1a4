package test;

import java.util.ArrayList;
import java.util.List;

import dao.CatDAO;
import model.Cat;

public class TestCat {
	public static void main(String[] args) {
		CatDAO cd1 = new CatDAO();
		
		List<Cat> list = cd1.selectAll();
		for (Cat c : list) {
			System.out.println(c.toString());
		}
	}
}
